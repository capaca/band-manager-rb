  <%= f.error_messages %>
  
  <table class="form">
    <tr>
      <td>
        <%= f.label 'E-mail' %><br/>
        <%= f.text_field :email %>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.label 'Senha' %><br/>
        <%= f.password_field :password %>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.label 'Confirmação da senha' %><br/>
        <%= f.password_field :password_confirmation %>
      </td>
    </tr>
  </table>

  <br />
  <%= f.submit "Cadastrar" %>
